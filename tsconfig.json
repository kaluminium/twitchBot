{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "types": ["node", "dotenv"],
      "skipDefaultLibCheck": true,
      "resolveJsonModule": true,
    },
    "include": ["src", "src/**/*.json", "types"]
  }
  